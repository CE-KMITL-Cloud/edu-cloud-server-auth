{
  "name": "edu-cloud-server-auth",
  "version": "0.0.1",
  "description": "edu-cloud-server-auth",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "dev": "ENV_NAME=development nest start --watch",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prepare": "husky install",
    "format": "prettier --write \"{src,pages,test}/**/*.{json,js,jsx,ts,tsx,html}\"",
    "lint": "eslint --quiet \"{src,pages}/**/*.{js,jsx,ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:ci": "jest --coverage --ci --reporters=default --reporters=jest-junit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "ENV_NAME=test jest --config ./test/jest-e2e.json --forceExit --runInBand --bail",
    "generate": "npx dotenv -e envs/.env.development -c development -- npx prisma generate",
    "db-push:dev": "npx dotenv -e envs/.env.development -c development -- npx prisma db push",
    "db-push:dev-reset": "npx dotenv -e envs/.env.development -c development -- npx prisma db push --force-reest",
    "db-push:test": "npx dotenv -e envs/.env.test -c test -- npx prisma db push",
    "db-pull:dev": "npx dotenv -e envs/.env.development -c development -- npx prisma db pull",
    "db-pull:test": "npx dotenv -e envs/.env.test -c test -- npx prisma db pull",
    "migrate:dev": "npx dotenv -e envs/.env.development -c development -- npx prisma migrate dev --create-only",
    "migrate:test": "npx dotenv -e envs/.env.test -c test -- npx prisma migrate dev",
    "register-student": "npx dotenv -e envs/.env.development -c development -- npx ts-node scripts/register.ts"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.3.0",
    "@prisma/client": "^4.12.0",
    "axios": "^1.3.6",
    "cache-manager": "3.4.4",
    "cache-manager-redis-store": "2.0.0",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.11.7",
    "dotenv-cli": "^7.1.0",
    "lodash": "^4.17.21",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "uuid": "^9.0.0",
    "web3": "^1.9.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/cache-manager": "3.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "18.15.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "prisma": "^4.12.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
